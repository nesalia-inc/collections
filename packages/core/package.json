{
  "name": "@deessejs/collections",
  "version": "0.0.1",
  "description": "High-level abstraction layer built on top of Drizzle ORM",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./fields": {
      "types": "./dist/fields/index.d.ts",
      "import": "./dist/fields/index.mjs",
      "require": "./dist/fields/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "drizzle-kit": "^0.22.0",
    "drizzle-orm": "^0.31.2",
    "pg": "^8.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@deessejs/type-testing": "^0.1.1",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.11.0",
    "eslint": "^8.57.0",
    "tsup": "^8.2.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "typescript": ">=5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
